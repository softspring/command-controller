{
    "name": "softspring/command-controller",
    "description": "This library allows running commands from Symfony controllers.",
    "license": "MIT",
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "symfony/console": "^4.0 || ^5.0",
        "symfony/framework-bundle": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CommandController\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CommandController\\Tests\\": "/Tests/"
        }
    },
    "conflict": {
        "symfony/framework-bundle": ">= 4.0.0 < 4.1.12"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
